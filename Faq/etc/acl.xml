<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Codelegacy_Faq::config" title="Config"/>
                <resource id="Codelegacy_Faq::main_content" title="FAQ">
                    <resource id="Codelegacy_Faq::category" title="Category">
                        <resource id="Codelegacy_Faq::category_save" title="Save category"/>
                        <resource id="Codelegacy_Faq::category_delete" title="Delete category"/>
                    </resource>
                    <resource id="Codelegacy_Faq::question" title="Question">
                        <resource id="Codelegacy_Faq::question_save" title="Save question"/>
                        <resource id="Codelegacy_Faq::question_delete" title="Delete question"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>